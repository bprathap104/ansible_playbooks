[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=ubuntu
Group=ubuntu
WorkingDirectory=/opt/myproject/myproject
ExecStart=/opt/myproject/env/bin/gunicorn --access-logfile - --workers 3 --bind unix:/opt/myproject/myproject.sock myproject.wsgi:application

[Install]
WantedBy=multi-user.target
